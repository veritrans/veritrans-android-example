    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CreditCardPaymentActivity" />
        <activity android:name=".PermataVAPaymentActivity" />
        <activity android:name=".MandiriBillPaymentActivity" />
        <activity android:name=".BCAPaymentActivity" />
        androidsdk-private-development-debug.aar
        <activity android:name=".MandiriClickPaymentActivity" />
        <activity android:name=".CIMCBClickPaymentActivity" />
        <activity android:name=".IndomaretPaymentActivity" />
        <activity android:name=".MandiriECashActivity" />
        <activity android:name=".BCAKlikPayActivity" />
        <activity android:name=".CardRegistrationActivity"></activity>
    </application>

</manifest>
